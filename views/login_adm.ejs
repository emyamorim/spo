<body>

<div class="header">
<img class="logo" src="imagens/logo.png" width="300" height="180">
</div>

<!-- partial:index.partial.html -->
<div class="container">
  <div class="formBox level-login">
    <div class="box boxShaddow"></div>
    <div class="box loginBox">
      <h2>ADMINISTRADOR</h2>
      <form class="form" action="/logar" method="post">
        <div class="f_row">
          <label>Login</label>
          <input name="login" type="text" class="input-field" required>
          <u></u>
        </div>
        <div class="f_row last">
          <label>Senha</label>
          <input name="senha" type="password" class="input-field" required>
          <u></u>
        </div>
   <button type="submit" class="btn-large">Entrar</button>
      </form>
    </div>
  
  </div>
</div>
